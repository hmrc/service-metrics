# microservice specific routes
GET        /:service/collections     uk.gov.hmrc.servicemetrics.controllers.MongoMetricsController.getCollections(service: String, environment: Option[Environment] ?= None)
GET        /:service/log-metrics     uk.gov.hmrc.servicemetrics.controllers.MongoMetricsController.getLogMetrics(service: String, from: java.time.Instant ?= java.time.Instant.now().minus(1, java.time.temporal.ChronoUnit.DAYS), to: java.time.Instant ?= java.time.Instant.now())
